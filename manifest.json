{
  "name": "ToMute",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Extension to mute Twitch when ads play!",
  "homepage_url": "https://github.com/amastov/ToMute",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "*://*.twitch.tv/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "src/inject/ToMute.js"
      ]
    }
  ]
}